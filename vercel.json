{
    "version": 2,
    "builds": [
        {
            "src": "Project/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "Project/uploads",
            "dest": "Project/app.py"
        },
        {
            "src": "Project/(.*)",
            "dest": "Project/app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production"
    }
}